<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 引用服务配置 -->
    <dubbo:reference id="testDubboApi" interface="com.wxj.testDubbo.service.TestDubboService" cluster="failfast" check="false"/>
    
    <!-- 登录相关处理的接口 -->
    <dubbo:reference id="loginApi" interface="com.wxj.loginAndResign.service.LoginAndResignService" timeout="1200000"/>
</beans>